#!/usr/bin/env python

import pexpect as expect
import sys

c = expect.spawn('telnet %s' % sys.argv[1])
for x in range(2):
    c.expect('password:')
    c.sendline('')
c.expect('password:')
c.sendline('hk2008')
c.expect('#')
c.logfile = sys.stdout

c.sendline('ps |grep -v "]$" |grep -v grep')
c.expect('#')
c.sendline('exit')
c.expect(expect.EOF)
c.close();

