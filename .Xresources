!!! XTerm <=== <=== <=== !!! <=== <=== <===
!xterm*termName: xterm-256color
! xterm.termName: xterm-256color
!% xterm.geometry: 110x54
xterm*scrollBar: false
xterm*rightScrollBar: true
xterm*loginshell: true
xterm*cursorBlink: true
xterm*background: black
xterm*foreground: lightgray
xterm.borderLess: true
xterm.cursorBlink: true
xterm*colorUL: yellow
xterm*colorBD: white
!### font and locale
xterm*locale: true
xterm.utf8: true
xterm*utf8Title: true
xterm*fontMenu*fontdefault*Label: Default
xterm*faceName: Monaco:antialias=True:pixelsize=18
!! xterm*faceName: monofur:antialias=True:pixelsize=20
!! xter*boldFont: DejaVu Sans Mono:style=Bold:pixelsize=15
!! xterm*faceNameDoublesize: wenquanyi bitmap song:pixelsize=16:antialias=True
xterm*faceNameDoublesize: Microsoft YaHei:pixelsize=18:antialias=True
xterm*xftAntialias: true
!! xterm*cjkWidth: false
!xterm*onXClicks: 4
!#xterm*on2Clicks: regex [a-zA-Z0-9_.:_\-\/]+
! xterm*on4Clicks: regex [^(){}\[\]]+
!###
!xterm*cutNewLine: false
!xterm*cutToBeginningOfLine: false
!xterm*charClass: 33:48,35:48,37:48,42:48,45-47:48,64:48,95:48,126:48
XTerm*VT100.cutNewLine: false
XTerm*VT100.cutToBeginningOfLine: false
XTerm*VT100.charClass: 33:48,35:48,37:48,42:48,45-47:48,64:48,95:48,126:48
!### fix alt key input
!!!xterm*eightBitInput: false
!!!xterm*altSendsEscape: true
!### print color and bold/underline attributes
!!!xterm*printAttributes: 2
!!!xterm*printerCommand: cat > ~/xtermdump

